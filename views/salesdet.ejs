  <h2 style="color:rgb(139,0,139)" style="font-family:Montserrat;">Sales Transactions</h2>
  <div class="main-wrap" id="bleh">
  <table id="salest">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Customer Name</th>
        <th scope="col">Item</th>
        <th scope="col">Quantity</th>
        <th scope="col">Date</th>
      </tr>
    </thead>
    <tbody>
      <%salesList.forEach(sales => {%>
        <tr>
            <th scope="row"><%= sales._id %></th>
            <td><%= sales.CustomerName %></td>
            <td><%= sales.item %></td>
            <td><%= sales.Quantity %></td>
            <td><%= sales.date %></td>
        </tr>
    <%})%> 
    </tbody>
  </table>
